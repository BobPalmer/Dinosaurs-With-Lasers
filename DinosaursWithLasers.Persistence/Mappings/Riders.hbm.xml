<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  assembly="DinosaursWithLasers.Model"
  namespace="DinosaursWithLasers.Model">
  <class name="Rider" table="Riders" dynamic-update="true">
    <id name="RiderId">
      <generator class="hilo"/>
    </id>
    <property name ="Name" />
    <property name ="FigImageUrl" />
    <many-to-one class="Dinosaur" name="DinoBox" column="DinosaurId"/>
  </class>
</hibernate-mapping>